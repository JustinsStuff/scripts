Frame.Size = UDim2.new(0.137898892, 0, 0.317045838, 0)
Frame.Draggable = true

UICorner.Parent = Frame

HeadText.Name = "HeadText"
HeadText.Parent = Frame
HeadText.BackgroundColor3 = Color3.fromRGB(49, 49, 49)
HeadText.BorderColor3 = Color3.fromRGB(0, 0, 0)
