TorsoSizer.Size = UDim2.new(0.462184876, 0, 0.200000003, 0)
TorsoSizer.Font = Enum.Font.SourceSans
TorsoSizer.Text = ""
TorsoSizer.TextColor3 = Color3.fromRGB(0, 255, 0)
TorsoSizer.TextSize = 14.000

UICorner_7.Parent = TorsoSizer

-- Scripts:

local function JTWGTIS_fake_script() -- Frame.Main 
	local script = Instance.new('LocalScript', Frame)

	local player = game.Players.LocalPlayer
	local user = game:GetService("UserInputService")
	local runService = game:GetService("RunService")
	local frame = script.Parent
	local headSizer = frame.HeadSizer
	local humanoidRootPartSizer = frame.HumanoidRootPartSizer
	local torsoSizer = frame.TorsoSizer
	local controlDown = false
	local eDown = false
	local bDown = false
	local hbeToggled = false
	local OriginalSizes = {}
	
	print("ctrl + e to activate hbe")
	print("ctrl + b to toggle gui")
	
	
	local function SetVictimsSizesR6(victimsCharacter, victim)
		local humanoidRootPart = victimsCharacter:FindFirstChild("HumanoidRootPart")
		local head = victimsCharacter:FindFirstChild("Head")
		local torso = victimsCharacter:FindFirstChild("Torso")
		if humanoidRootPart then
			if humanoidRootPartSizer.Text == "" then
				if OriginalSizes[victim.Name]["HumanoidRootPart"] and OriginalSizes[victim.Name]["HumanoidRootPart"] ~= "" then
					humanoidRootPart.Size = OriginalSizes[victim.Name]["HumanoidRootPart"]
					humanoidRootPart.Transparency = 1
				end
			elseif humanoidRootPartSizer.Text ~= "" then
				if victim and victim.Team then
					humanoidRootPart.BrickColor = victim.TeamColor
				else
					humanoidRootPart.BrickColor = BrickColor.new("White")
				end
				humanoidRootPart.Material = Enum.Material.SmoothPlastic
				humanoidRootPart.CanCollide = false
				humanoidRootPart.Transparency = 0.65
				humanoidRootPart.Size = Vector3.new(humanoidRootPartSizer.Text,humanoidRootPartSizer.Text,humanoidRootPartSizer.Text)
			end
		end
		if head then
			if headSizer.Text == "" then
				if OriginalSizes[victim.Name]["Head"] and OriginalSizes[victim.Name]["Head"] ~= "" then
					head.Size = OriginalSizes[victim.Name]["Head"]
					head.Transparency = 0
				end
			elseif headSizer.Text ~= "" then
				if victim and victim.Team then
					head.BrickColor = victim.TeamColor
				else
					head.BrickColor = BrickColor.new("White")
				end
				head.CanCollide = false
				head.Transparency = 0.65
				head.Massless = true
				head.Size = Vector3.new(headSizer.Text,headSizer.Text,headSizer.Text)
			end
		end
		if torso then
			if torsoSizer.Text == "" then
				if OriginalSizes[victim.Name]["Torso"] and OriginalSizes[victim.Name]["Torso"] ~= "" then
					torso.Size = OriginalSizes[victim.Name]["Torso"]
					torso.Transparency = 0
				end
			elseif torsoSizer.Text ~= "" then
				torso.CanCollide = false
				torso.Transparency = 0.65
				torso.Massless = true
				torso.Size = Vector3.new(torsoSizer.Text,torsoSizer.Text,torsoSizer.Text)
			end
		end
	end
	
	
	local function SetVictimsSizesR15(victimsCharacter, victim)
		local humanoidRootPart = victimsCharacter:FindFirstChild("HumanoidRootPart")
		local head = victimsCharacter:FindFirstChild("Head")
		local upperTorso = victimsCharacter:FindFirstChild("UpperTorso")
		local lowerTorso = victimsCharacter:FindFirstChild("LowerTorso")
		if humanoidRootPart then
			if humanoidRootPartSizer.Text == "" then
				if OriginalSizes[victim.Name]["HumanoidRootPart"] and OriginalSizes[victim.Name]["HumanoidRootPart"] ~= "" then
					humanoidRootPart.Size = OriginalSizes[victim.Name]["HumanoidRootPart"]
					humanoidRootPart.Transparency = 1
				end
			elseif humanoidRootPartSizer.Text ~= "" then
				if victim and victim.Team then
					humanoidRootPart.BrickColor = victim.TeamColor
				else
					humanoidRootPart.BrickColor = BrickColor.new("White")
				end
				humanoidRootPart.Material = Enum.Material.SmoothPlastic
				humanoidRootPart.CanCollide = false
				humanoidRootPart.Transparency = 0.65
				humanoidRootPart.Size = Vector3.new(humanoidRootPartSizer.Text,humanoidRootPartSizer.Text,humanoidRootPartSizer.Text)
			end
		end
		if head then
			if headSizer.Text == "" then
				if OriginalSizes[victim.Name]["Head"] and OriginalSizes[victim.Name]["Head"] ~= "" then
					head.Size = OriginalSizes[victim.Name]["Head"]
					head.Transparency = 0
				end
			elseif headSizer.Text ~= "" then
				if victim and victim.Team then
					head.BrickColor = victim.TeamColor
				else
					head.BrickColor = BrickColor.new("White")
				end
				head.CanCollide = false
				head.Transparency = 0.65
				head.Massless = true
				head.Size = Vector3.new(headSizer.Text,headSizer.Text,headSizer.Text)
			end
		end
		if upperTorso then
			if torsoSizer.Text == "" then
				if OriginalSizes[victim.Name]["UpperTorso"] and OriginalSizes[victim.Name]["UpperTorso"] ~= "" then
					upperTorso.Size = OriginalSizes[victim.Name]["UpperTorso"]
					upperTorso.Transparency = 0
				end
			elseif torsoSizer.Text ~= "" then
				upperTorso.CanCollide = false
				upperTorso.Transparency = 0.65
				upperTorso.Massless = true
				upperTorso.Size = Vector3.new(torsoSizer.Text,torsoSizer.Text,torsoSizer.Text)
			end
		end
		if lowerTorso then
			if torsoSizer.Text == "" then
				if OriginalSizes[victim.Name]["LowerTorso"] and OriginalSizes[victim.Name]["LowerTorso"] ~= "" then
					lowerTorso.Size = OriginalSizes[victim.Name]["LowerTorso"]
					lowerTorso.Transparency = 0
				end
			elseif torsoSizer.Text ~= "" then
				lowerTorso.CanCollide = false
				lowerTorso.Transparency = 0.65
				lowerTorso.Massless = true
				lowerTorso.Size = Vector3.new(torsoSizer.Text,torsoSizer.Text,torsoSizer.Text)
			end
		end
	end
	
	
	user.InputBegan:Connect(function(key, typing)
		if key.KeyCode == Enum.KeyCode.LeftControl then
			controlDown = true
		end
		if key.KeyCode == Enum.KeyCode.B then
			bDown = true
		end
		if key.KeyCode == Enum.KeyCode.E then
			eDown = true
			if controlDown and eDown then
				hbeToggled = not hbeToggled
				if not hbeToggled then
					stepped:Disconnect()
				end
			end
		end
		if controlDown and bDown then
			frame.Visible = not frame.Visible
		end
	end)
	
	stepped = runService.Stepped:Connect(function()
		if hbeToggled then
			for _, victim in pairs(game:GetService("Players"):GetPlayers()) do
				if victim.Name == player.Name then continue end
				if victim and victim.Character and victim.Character:FindFirstChild("Humanoid") and victim.Character:FindFirstChild("Humanoid").Health > 0 then
					local victimsCharacter = victim.Character
					if victimsCharacter.Humanoid.RigType == Enum.HumanoidRigType.R6 then
						if not OriginalSizes[victim.Name] then
							OriginalSizes[victim.Name] = {}
						end
						if victimsCharacter:FindFirstChild("Head") then
							if not OriginalSizes[victim.Name]["Head"] then
								OriginalSizes[victim.Name]["Head"] = victimsCharacter:FindFirstChild("Head").Size
							end
						end
						if victimsCharacter:FindFirstChild("HumanoidRootPart") then
							if not OriginalSizes[victim.Name]["HumanoidRootPart"] then 
								OriginalSizes[victim.Name]["HumanoidRootPart"] = victimsCharacter:FindFirstChild("HumanoidRootPart").Size
							end
						end
						if victimsCharacter:FindFirstChild("Torso") then
							if not OriginalSizes[victim.Name]["Torso"] then
								OriginalSizes[victim.Name]["Torso"] = victimsCharacter:FindFirstChild("Torso").Size
							end
						end
						SetVictimsSizesR6(victimsCharacter, victim)
					elseif victimsCharacter.Humanoid.RigType == Enum.HumanoidRigType.R15 then
						if not OriginalSizes[victim.Name] then
							OriginalSizes[victim.Name] = {}
						end
						if victimsCharacter:FindFirstChild("Head") then
							if not OriginalSizes[victim.Name]["Head"] then
								OriginalSizes[victim.Name]["Head"] = victimsCharacter:FindFirstChild("Head").Size
							end
						end
						if victimsCharacter:FindFirstChild("HumanoidRootPart") then
							if not OriginalSizes[victim.Name]["HumanoidRootPart"] then 
								OriginalSizes[victim.Name]["HumanoidRootPart"] = victimsCharacter:FindFirstChild("HumanoidRootPart").Size
							end
						end
						if victimsCharacter:FindFirstChild("UpperTorso") then
							if not OriginalSizes[victim.Name]["UpperTorso"] then
								OriginalSizes[victim.Name]["UpperTorso"] = victimsCharacter:FindFirstChild("UpperTorso").Size
							end
						end
						if victimsCharacter:FindFirstChild("LowerTorso") then
							if not OriginalSizes[victim.Name]["LowerTorso"] then
								OriginalSizes[victim.Name]["LowerTorso"] = victimsCharacter:FindFirstChild("LowerTorso").Size
							end
						end
						SetVictimsSizesR15(victimsCharacter, victim)
					end
				end
			end
		end
	end)
	
	user.InputEnded:Connect(function(key, typing)
		if key.KeyCode == Enum.KeyCode.LeftControl then
			controlDown = false
		end
		if key.KeyCode == Enum.KeyCode.E then
			eDown = false
		end
		if key.KeyCode == Enum.KeyCode.B then
			bDown = false
		end
	end)
end
coroutine.wrap(JTWGTIS_fake_script)()
