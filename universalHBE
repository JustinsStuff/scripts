local Frame = Instance.new("Frame")
local UICorner = Instance.new("UICorner")
local HeadText = Instance.new("TextLabel")
local UICorner_2 = Instance.new("UICorner")
local HumanoidRootPartText = Instance.new("TextLabel")
local UICorner_3 = Instance.new("UICorner")
local TorsoText = Instance.new("TextLabel")
local UICorner_4 = Instance.new("UICorner")
local HeadSizer = Instance.new("TextBox")
local UICorner_5 = Instance.new("UICorner")
local HumanoidRootPartSizer = Instance.new("TextBox")
local UICorner_6 = Instance.new("UICorner")
local TorsoSizer = Instance.new("TextBox")
local UICorner_7 = Instance.new("UICorner")

--Properties:

Hit.Name = "Hit"
Hit.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
Hit.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
Hit.ResetOnSpawn = false

Frame.Parent = Hit
Frame.Active = true
Frame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
Frame.BorderColor3 = Color3.fromRGB(0, 0, 0)
Frame.BorderSizePixel = 0
