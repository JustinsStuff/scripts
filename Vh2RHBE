-- Scripts:

local function PGMSFYQ_fake_script() -- MainGUI.Main 
	local script = Instance.new('LocalScript', MainGUI)

	-- gui stuff
	local MainGUI = script.Parent
	local buttonsFrame = MainGUI.ButtonsFrame
	local tabFrame = MainGUI.TabFrame
	local titleFrame = MainGUI.TitleFrame
	local espTab = buttonsFrame.ESPTab
	local hitboxTab = tabFrame.HitboxTab
	local autoEscapeTab = buttonsFrame.AutoEscapeTab
	local hitboxTabScroll = hitboxTab.HitboxTabScroll
	local hitboxSizer = hitboxTabScroll.HitboxSizer
	local hitboxTransparency = hitboxTabScroll.HitboxTransparency
	local hitboxColorRed = hitboxTabScroll.HitboxColorRed
	local hitboxColorGreen = hitboxTabScroll.HitboxColorGreen
	local hitboxColorBlue = hitboxTabScroll.HitboxColorBlue
	local vampireCheck = hitboxTabScroll.VampireCheck
	local hitboxToggle = hitboxTabScroll.HitboxToggle
	-- hbe script stuff
	local me = game.Players.LocalPlayer
	local runService = game:GetService("RunService")
	local user = game:GetService("UserInputService")
	local toggled = false
	local vampToggled = false
	local controlDown = false
	local eDown = false
	local bDown = false
	local stepped
	local originalSize = Vector3.new(2, 2, 1)
	local originalSizeR15 = Vector3.new(2, 1.6000003814697266, 1)
	local debris = game:GetService("Debris")
	
	
	vampireCheck.MouseButton1Click:Connect(function()
		vampToggled = not vampToggled
		if vampToggled then
			vampireCheck.Text = "ON"
			vampireCheck.TextColor = BrickColor.new("Lime green")
		elseif not vampToggled then
			vampireCheck.Text = "OFF"
			vampireCheck.TextColor = BrickColor.new("Really red")
		end
	end)
	
	hitboxToggle.MouseButton1Click:Connect(function()
		toggled = not toggled
		if toggled then
			hitboxToggle.Text = "ON"
			hitboxToggle.TextColor = BrickColor.new("Lime green")
		elseif not toggled then
			hitboxToggle.Text = "OFF"
			hitboxToggle.TextColor = BrickColor.new("Really red")
		end
	end)
	
	
	local function RemoveHBE(character)
	if character and character:FindFirstChild("Humanoid") and character:FindFirstChild("Humanoid").RigType == Enum.HumanoidRigType.R6 then
		local rootPart = character:FindFirstChild("12")
		if rootPart then
			local torso = character:FindFirstChild("Torso")
			if torso then
				rootPart.Size = originalSize
				rootPart.Name = "Torso"
				debris:AddItem(torso, 0)
				rootPart.CanCollide = true
				rootPart.Massless = false
				rootPart.Transparency = 0
				rootPart.Color = character:FindFirstChild("Head").Color
			end
		end
	elseif character and character:FindFirstChild("Humanoid") and character:FindFirstChild("Humanoid").RigType == Enum.HumanoidRigType.R15 then
		local rootPart = character:FindFirstChild("12")
		if rootPart then
			local torso = character:FindFirstChild("UpperTorso")
			if torso then
				rootPart.Size = originalSize
				rootPart.Name = "UpperTorso"
				debris:AddItem(torso, 0)
				rootPart.CanCollide = true
				rootPart.Massless = false
				rootPart.Transparency = 0
				rootPart.Color = character:FindFirstChild("Head").Color
			end
		end
	end
end
	
	local function SetHBE(character)
	if character and character:FindFirstChild("Humanoid") and character:FindFirstChild("Humanoid").RigType == Enum.HumanoidRigType.R6 then
		if vampToggled then
			if character:FindFirstChild("charvalues") then
				local charvalues = character:FindFirstChild("charvalues")
				if charvalues:FindFirstChild("vampire") then
					local vampire = charvalues:FindFirstChild("vampire")
					if vampire.Value == false then
						local characterL = character
						RemoveHBE(characterL)
					elseif vampire.Value == true then
						local torso = character:FindFirstChild("Torso") -- sets hbe for the torso
						if torso and not character:FindFirstChild("12") then
							local clonedTorso = torso:Clone()
							clonedTorso.Parent = character
							clonedTorso.CanQuery = false
							torso.Name = "12"
							torso.CanCollide = false
							torso.Massless = true
							if hitboxTransparency.Text and hitboxTransparency.Text ~= "" then
								character:FindFirstChild("12").Transparency = hitboxTransparency.Text
							end
							torso.Size = Vector3.new(hitboxSizer.Text,hitboxSizer.Text,hitboxSizer.Text)
							torso.Material = Enum.Material.SmoothPlastic
						end
						if torso and character:FindFirstChild("12") then
							if hitboxTransparency.Text and hitboxTransparency.Text ~= "" then
								character:FindFirstChild("12").Transparency = hitboxTransparency.Text
							end
							character:FindFirstChild("12").Color = Color3.new(hitboxColorRed.Text, hitboxColorGreen.Text, hitboxColorBlue.Text)
							character:FindFirstChild("12").CanCollide = false
							character:FindFirstChild("12").Size = Vector3.new(hitboxSizer.Text,hitboxSizer.Text,hitboxSizer.Text)
							character:FindFirstChild("12").Massless = true
						end
					end
				end
			end
		elseif not vampToggled then
		-- hello this is the default set hbe
			local torso = character:FindFirstChild("Torso") -- sets hbe for the torso
			if torso and not character:FindFirstChild("12") then
				local clonedTorso = torso:Clone()
				clonedTorso.Parent = character
				clonedTorso.CanQuery = false
				torso.Name = "12"
				torso.CanCollide = false
				torso.Massless = true
				if hitboxTransparency.Text and hitboxTransparency.Text ~= "" then
					character:FindFirstChild("12").Transparency = hitboxTransparency.Text
				end
				torso.Size = Vector3.new(hitboxSizer.Text,hitboxSizer.Text,hitboxSizer.Text)
				torso.Material = Enum.Material.SmoothPlastic
			end
			if torso and character:FindFirstChild("12") then
				if hitboxTransparency.Text and hitboxTransparency.Text ~= "" then
					character:FindFirstChild("12").Transparency = hitboxTransparency.Text
				end
				character:FindFirstChild("12").Color = Color3.new(hitboxColorRed.Text, hitboxColorGreen.Text, hitboxColorBlue.Text)
				character:FindFirstChild("12").CanCollide = false
				character:FindFirstChild("12").Size = Vector3.new(hitboxSizer.Text,hitboxSizer.Text,hitboxSizer.Text)
				character:FindFirstChild("12").Massless = true
			end
		end
elseif character and character:FindFirstChild("Humanoid") and character:FindFirstChild("Humanoid").RigType == Enum.HumanoidRigType.R15 then
		if vampToggled then
			if character:FindFirstChild("charvalues") then
				local charvalues = character:FindFirstChild("charvalues")
				if charvalues:FindFirstChild("vampire") then
					local vampire = charvalues:FindFirstChild("vampire")
					if vampire.Value == false then
						local characterL = character
						RemoveHBE(characterL)
					elseif vampire.Value == true then
						local torso = character:FindFirstChild("UpperTorso") -- sets hbe for the torso
						if torso and not character:FindFirstChild("12") then
							local clonedTorso = torso:Clone()
							clonedTorso.Parent = character
							clonedTorso.CanQuery = false
							torso.Name = "12"
							torso.CanCollide = false
							torso.Massless = true
							if hitboxTransparency.Text and hitboxTransparency.Text ~= "" then
								character:FindFirstChild("12").Transparency = hitboxTransparency.Text
							end
							torso.Size = Vector3.new(hitboxSizer.Text,hitboxSizer.Text,hitboxSizer.Text)
							torso.Material = Enum.Material.SmoothPlastic
						end
						if torso and character:FindFirstChild("12") then
							if hitboxTransparency.Text and hitboxTransparency.Text ~= "" then
								character:FindFirstChild("12").Transparency = hitboxTransparency.Text
							end
							character:FindFirstChild("12").Color = Color3.new(hitboxColorRed.Text, hitboxColorGreen.Text, hitboxColorBlue.Text)
							character:FindFirstChild("12").CanCollide = false
							character:FindFirstChild("12").Size = Vector3.new(hitboxSizer.Text,hitboxSizer.Text,hitboxSizer.Text)
							character:FindFirstChild("12").Massless = true
						end
					end
				end
			end
		elseif not vampToggled then
		-- hello this is the r15 set hbe
			local torso = character:FindFirstChild("UpperTorso") -- sets hbe for the torso
			if torso and not character:FindFirstChild("12") then
				local clonedTorso = torso:Clone()
				clonedTorso.Parent = character
				clonedTorso.CanQuery = false
				torso.Name = "12"
				torso.CanCollide = false
				torso.Massless = true
				if hitboxTransparency.Text and hitboxTransparency.Text ~= "" then
					character:FindFirstChild("12").Transparency = hitboxTransparency.Text
				end
				torso.Size = Vector3.new(hitboxSizer.Text,hitboxSizer.Text,hitboxSizer.Text)
				torso.Material = Enum.Material.SmoothPlastic
			end
			if torso and character:FindFirstChild("12") then
				if hitboxTransparency.Text and hitboxTransparency.Text ~= "" then
					character:FindFirstChild("12").Transparency = hitboxTransparency.Text
				end
				character:FindFirstChild("12").Color = Color3.new(hitboxColorRed.Text, hitboxColorGreen.Text, hitboxColorBlue.Text)
				character:FindFirstChild("12").CanCollide = false
				character:FindFirstChild("12").Size = Vector3.new(hitboxSizer.Text,hitboxSizer.Text,hitboxSizer.Text)
				character:FindFirstChild("12").Massless = true
			end
		end
	end
end
		
	
	runService.Stepped:Connect(function()
		for _, player in pairs(game:GetService("Players"):GetPlayers()) do
			if player.Name == me.Name then continue end
			if player.Character then
				if hitboxSizer.Text ~= "" then
					if toggled then
						if hitboxToggle.Text == "OFF" and hitboxToggle.TextColor == BrickColor.new("Really red") then
							hitboxToggle.Text = "ON"
							hitboxToggle.TextColor = BrickColor.new("Lime green")
						end
						SetHBE(player.Character)
					else
						if hitboxToggle.Text == "ON" and hitboxToggle.TextColor == BrickColor.new("Lime green") then
							hitboxToggle.Text = "OFF"
							hitboxToggle.TextColor = BrickColor.new("Really red")
						end
						RemoveHBE(player.Character)
					end
				end
			end
		end
	end)
	
	user.InputBegan:Connect(function(key, typing)
		if key.KeyCode == Enum.KeyCode.LeftControl then
			controlDown = true
		end
		if key.KeyCode == Enum.KeyCode.E then
			eDown = true
			if controlDown and eDown then
				toggled = not toggled
				if toggled then
					if hitboxToggle.Text == "OFF" and hitboxToggle.TextColor == BrickColor.new("Really red") then
						hitboxToggle.Text = "ON"
						hitboxToggle.TextColor = BrickColor.new("Lime green")
					end
				else
					if hitboxToggle.Text == "ON" and hitboxToggle.TextColor == BrickColor.new("Lime green") then
						hitboxToggle.Text = "OFF"
						hitboxToggle.TextColor = BrickColor.new("Really red")
					end
				end
			end
		end
		if key.KeyCode == Enum.KeyCode.B then
			bDown = true
			if controlDown and bDown then
				MainGUI.Visible = not MainGUI.Visible
			end
		end
	end)
	
	user.InputEnded:Connect(function(key, typing)
		if key.KeyCode == Enum.KeyCode.LeftControl then
			controlDown = false
		end
		if key.KeyCode == Enum.KeyCode.E then
			eDown = false
		end
		if key.KeyCode == Enum.KeyCode.B then
			bDown = false
		end
	end)
end
coroutine.wrap(PGMSFYQ_fake_script)()
