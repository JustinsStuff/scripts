-- Scripts:

local function HIXT_fake_script() -- ToggleOnOff.ToggleOnOff 
	local script = Instance.new('LocalScript', ToggleOnOff)

	local toggleButton = script.Parent
	local toggled = false

	toggleButton.MouseButton1Click:Connect(function()
		toggled = not toggled
		if toggled then
			toggleButton.Text = "On"
			toggleButton.TextColor = BrickColor.new("Lime green")
		elseif not toggled then
			toggleButton.Text = "Off"
			toggleButton.TextColor = BrickColor.new("Really red")
		end
	end)
end
coroutine.wrap(HIXT_fake_script)()
local function LQECCDC_fake_script() -- Frame.MainScript 
	local script = Instance.new('LocalScript', Frame)

	local player = game.Players.LocalPlayer
	local user = game:GetService("UserInputService")
	local frame = script.Parent
	local scrollingFrame = frame.ScrollingFrame
	local forwardForcer = scrollingFrame.ForwardForcer
	local upForcer = scrollingFrame.UpForcer
	local downForcer = scrollingFrame.DownForcer
	local activateHotkeyBox = scrollingFrame.ActivateHotkey
	local slowdownHotkeyBox = scrollingFrame.SlowdownHotkey
	local anchorHotkeyBox = scrollingFrame.AnchorHotkey
	local toggleButton = script.Parent.ToggleOnOff
	local activateHotkey
	local slowdownHotkey
	local anchorHotkey
	local ctrlDown = false
	local bDown = false


	forwardForcer.Text = 50
	upForcer.Text = 0
	downForcer.Text = 0


	activateHotkey = Enum.KeyCode.E
	slowdownHotkey = Enum.KeyCode.Q
	anchorHotkey = Enum.KeyCode.Three
	activateHotkeyBox.Text = "E"
	slowdownHotkeyBox.Text = "Q"
	anchorHotkeyBox.Text = "3"


	activateHotkeyBox.Changed:Connect(function() -- updates activate hotkey
		if activateHotkeyBox and activateHotkeyBox.Text ~= "" then
			local letter = string.upper(activateHotkeyBox.Text)
			if activateHotkeyBox.Text >= "A" and activateHotkeyBox.Text <= "Z" then
				activateHotkey = Enum.KeyCode[letter]
				return
			elseif activateHotkeyBox.Text >= "a" and activateHotkeyBox.Text <= "z" then
				activateHotkey = Enum.KeyCode[letter]
				return
			elseif activateHotkeyBox.Text == "1" then
				activateHotkey = Enum.KeyCode.One
				return
			elseif activateHotkeyBox.Text == "2" then
				activateHotkey = Enum.KeyCode.Two
				return
			elseif activateHotkeyBox.Text == "3" then
				activateHotkey = Enum.KeyCode.Three
				return
			elseif activateHotkeyBox.Text == "4" then
				activateHotkey = Enum.KeyCode.Four
				return
			elseif activateHotkeyBox.Text == "5" then
				activateHotkey = Enum.KeyCode.Five
				return
			elseif activateHotkeyBox.Text == "6" then
				activateHotkey = Enum.KeyCode.Six
				return
			elseif activateHotkeyBox.Text == "7" then
				activateHotkey = Enum.KeyCode.Seven
				return
			elseif activateHotkeyBox.Text == "8" then
				activateHotkey = Enum.KeyCode.Nine
				return
			elseif activateHotkeyBox.Text == "9" then
				activateHotkey = Enum.KeyCode.Nine
				return
			elseif activateHotkeyBox.Text == "0" then
				activateHotkey = Enum.KeyCode.Zero
				return
			end
		end
	end)


	slowdownHotkeyBox.Changed:Connect(function() -- updates slowdown hotkey
		if slowdownHotkeyBox and slowdownHotkeyBox.Text ~= "" then
			local letter = string.upper(slowdownHotkeyBox.Text)
			if slowdownHotkeyBox.Text >= "A" and slowdownHotkeyBox.Text <= "Z" then
				slowdownHotkey = Enum.KeyCode[letter]
				return
			elseif slowdownHotkeyBox.Text >= "a" and slowdownHotkeyBox.Text <= "z" then
				slowdownHotkey = Enum.KeyCode[letter]
				return
			elseif slowdownHotkeyBox.Text == "1" then
				slowdownHotkey = Enum.KeyCode.One
				return
			elseif slowdownHotkeyBox.Text == "2" then
				slowdownHotkey = Enum.KeyCode.Two
				return
			elseif slowdownHotkeyBox.Text == "3" then
				slowdownHotkey = Enum.KeyCode.Three
				return
			elseif slowdownHotkeyBox.Text == "4" then
				slowdownHotkey = Enum.KeyCode.Four
				return
			elseif slowdownHotkeyBox.Text == "5" then
				slowdownHotkey = Enum.KeyCode.Five
				return
			elseif slowdownHotkeyBox.Text == "6" then
				slowdownHotkey = Enum.KeyCode.Six
				return
			elseif slowdownHotkeyBox.Text == "7" then
				slowdownHotkey = Enum.KeyCode.Seven
				return
			elseif slowdownHotkeyBox.Text == "8" then
				slowdownHotkey = Enum.KeyCode.Eight
				return
			elseif slowdownHotkeyBox.Text == "9" then
				slowdownHotkey = Enum.KeyCode.Nine
				return
			elseif slowdownHotkeyBox.Text == "0" then
				slowdownHotkey = Enum.KeyCode.Zero
				return
			end
		end
	end)


	anchorHotkeyBox.Changed:Connect(function() -- updates anchor hotkey
		if anchorHotkeyBox and anchorHotkeyBox.Text ~= "" then
			local letter = string.upper(anchorHotkeyBox.Text)
			if anchorHotkeyBox.Text >= "A" and anchorHotkeyBox.Text <= "Z" then
				anchorHotkey = Enum.KeyCode[letter]
				return
			elseif anchorHotkeyBox.Text >= "a" and anchorHotkeyBox.Text <= "z" then
				anchorHotkey = Enum.KeyCode[letter]
				return
			elseif anchorHotkeyBox.Text == "1" then
				anchorHotkey = Enum.KeyCode.One
				return
			elseif anchorHotkeyBox.Text == "2" then
				anchorHotkey = Enum.KeyCode.Two
				return
			elseif anchorHotkeyBox.Text == "3" then
				anchorHotkey = Enum.KeyCode.Three
				return
			elseif anchorHotkeyBox.Text == "4" then
				anchorHotkey = Enum.KeyCode.Four
				return
			elseif anchorHotkeyBox.Text == "5" then
				anchorHotkey = Enum.KeyCode.Five
				return
			elseif anchorHotkeyBox.Text == "6" then
				anchorHotkey = Enum.KeyCode.Six
				return
			elseif anchorHotkeyBox.Text == "7" then
				anchorHotkey = Enum.KeyCode.Seven
				return
			elseif anchorHotkeyBox.Text == "8" then
				anchorHotkey = Enum.KeyCode.Eight
				return
			elseif anchorHotkeyBox.Text == "9" then
				anchorHotkey = Enum.KeyCode.Nine
				return
			elseif anchorHotkeyBox.Text == "0" then
				anchorHotkey = Enum.KeyCode.Zero
				return
			end
		end
	end)

	upForcer.Changed:Connect(function()
		if upForcer.Text ~= "" then return end
		task.wait(3)
		if upForcer.Text == "" then upForcer.Text = 0 end
	end)


	downForcer.Changed:Connect(function()
		if downForcer.Text ~= "" then return end
		task.wait(3)
		if downForcer.Text == "" then downForcer.Text = 0 end
	end)


	forwardForcer.Changed:Connect(function()
		if forwardForcer.Text ~= "" then return end
		task.wait(3)
		if forwardForcer.Text == "" then forwardForcer.Text = 0 end
	end)


	user.InputBegan:Connect(function(key, typing)
		if key.KeyCode == Enum.KeyCode.LeftControl then
			ctrlDown = true
		end
	end)


	user.InputEnded:Connect(function(key, typing)
		if key.KeyCode == Enum.KeyCode.LeftControl then
			ctrlDown = false
		end
	end)


	user.InputBegan:Connect(function(key, typing)
		if key.KeyCode == Enum.KeyCode.B then
			bDown = true
			if ctrlDown and bDown then
				frame.Visible = not frame.Visible
			end
		end
	end)


	user.InputBegan:Connect(function(key, typing)
		if key.KeyCode == Enum.KeyCode.B then
			bDown = false
		end
	end)

	user.InputBegan:Connect(function(key, typing)
		if typing then return end
		if toggleButton.Text == "Off" then return end
		if activateHotkey and key.KeyCode == activateHotkey then -- if activate hotkey, then use forces.
			local character = player.Character
			local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
			if character and humanoidRootPart then
				if forwardForcer.Text ~= "" then
					character["HumanoidRootPart"].Velocity += character["HumanoidRootPart"].CFrame.LookVector * Vector3.new(forwardForcer.Text, 0, forwardForcer.Text)
				end
				if upForcer.Text ~= "" and downForcer.Text ~= "" then
					character["HumanoidRootPart"].Velocity += character["HumanoidRootPart"].CFrame.LookVector * Vector3.new(0,0,0) + Vector3.new(0, upForcer.Text - downForcer.Text, 0)
				end
			end
		elseif slowdownHotkey and key.KeyCode == slowdownHotkey then -- elseif its not the activate hotkey, and its the slow hotkey, then slow down.
			local character = player.Character
			local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
			if character and humanoidRootPart then
				character["HumanoidRootPart"].Velocity -= character["HumanoidRootPart"].Velocity
			end
		elseif anchorHotkey and key.KeyCode == anchorHotkey then -- elseif not either, then anchor player.
			local character = player.Character
			local head = character:FindFirstChild("HumanoidRootPart")
			if character and head then
				character["HumanoidRootPart"].Anchored = not character["HumanoidRootPart"].Anchored
			end
		end
	end)
end
coroutine.wrap(LQECCDC_fake_script)()
